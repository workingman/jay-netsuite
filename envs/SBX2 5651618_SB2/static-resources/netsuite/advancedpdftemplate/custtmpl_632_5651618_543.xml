<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width:100%;">
<thead>
	<tr>
	<th rowspan="3" scope="col"><#if companyInformation.logoUrl?length != 0><img src="https://5651618.app.netsuite.com/core/media/media.nl?id=16434&amp;c=5651618&amp;h=W_PI8S_d33T2FQ0atFslRolNnzdJaXOr0KzP5bCNydQz_h6k&amp;fcts=20221024135333&amp;whence=" style="margin: 4px 7px; width: 170px; height: 61px;" /> </#if></th>
	<th scope="col" style="text-align: right;"><span class="nameandaddress">${companyInformation.addressText}</span></th>
	</tr>
</thead><tr>
	<td align="right">&nbsp;</td>
	</tr>
	<tr>
	<td align="right">&nbsp;</td>
	</tr></table>
        </macro>
        <macro id="nlfooter">
            <table class="footer" style="width: 100%;"><tr>
	<td style="width: 509px;">SUBJECT TO PURCHASE TERMS AND CONDITIONS MAY BE FOUND AT:<br /><a href="https://thomasfoodsusa.com/terms-of-use/">https://thomasfoodsusa.com/terms-of-use/</a></td>
	<td align="right" style="width: 203px;"><pagenumber/> of <totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
		<#if .locale == "zh_CN">
			font-family: NotoSans, NotoSansCJKsc, sans-serif;
		<#elseif .locale == "zh_TW">
			font-family: NotoSans, NotoSansCJKtc, sans-serif;
		<#elseif .locale == "ja_JP">
			font-family: NotoSans, NotoSansCJKjp, sans-serif;
		<#elseif .locale == "ko_KR">
			font-family: NotoSans, NotoSansCJKkr, sans-serif;
		<#elseif .locale == "th_TH">
			font-family: NotoSans, NotoSansThai, sans-serif;
		<#else>
			font-family: NotoSans, sans-serif;
		</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
            color: #333333;
        }
        td {
            padding: 4px 6px;
        }
		td p { align:left }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 10px;
            padding-top: 10px;
        }
        table.body td {
            padding-top: 2px;
        }
        table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 8pt;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
        span.title {
            font-size: 28pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="10%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <table style="width: 640px;"><tr>
	<td class="addressheader" colspan="6" style="height: 38px; width: 346px;"><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:14px;"><span style="color:#16a085;"><strong>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;</strong></span><strong>Receiving Document</strong></span></span></td>
	<td class="addressheader" style="height: 38px; width: 285px;"><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;">PO Number:&nbsp;${record.tranid}<br />PO Date:&nbsp;${record.trandate}</span></span><br /><span style="font-size:11px;"><span style="font-family:Arial,Helvetica,sans-serif;">Total Quantity:&nbsp;</span></span>${record.custbody_pcg_total_cases}<br /><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;">Delivery Date:&nbsp;</span></span>${record.custbodytfi_delivery_date}</td>
	</tr>
	<tr>
	<td class="addressheader" colspan="6" style="height: 75px; width: 346px;">
	<div><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;"><span style="color:#000000;">Vendor:</span><br />${record.billaddress}</span></span></div>
	</td>
	<td class="addressheader" style="height: 75px; width: 285px;">Ship&nbsp;To:<br />${record.shipaddress}</td>
	</tr>
	<tr>
	<td class="address" colspan="6" rowspan="1" style="width: 346px;">
	<p><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;">Vessel:&nbsp;</span></span>${record.custbody_pcg_vessel}<br /><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;">Container:&nbsp;</span></span>${record.custbody_pcg_container_num}<br /><span style="font-family:Arial,Helvetica,sans-serif;"><span style="font-size:11px;">Comments:&nbsp;</span></span>${record.memo}</p>
	</td>
	<td class="address" rowspan="1" style="width: 285px;">BOL:&nbsp;${record.custbody_pcg_bol_num}<br />Health Cert:&nbsp;${record.custbody_pcg_tif_health_certificate}</td>
	</tr></table>
<#if record.item?has_content>

<table class="itemtable"><!-- start items --><#list record.item as item><#if item_index==0><#assign casecount = 0><#assign netweight = 0>
<thead>
	<tr>
	<th colspan="3" style="width: 100px; text-align: left;"><span style="font-size:10px;">Product<br /><br />Lot</span></th>
	<th colspan="3" style="width: 250px; text-align: left;"><span style="font-size:10px;">Description/Vendor Code</span></th>
	<th style="width: 60px;"><span style="font-size:10px;">Box Code</span></th>
	<th style="width: 70px;"><span style="font-size:10px;">Shipping<br />Mark</span></th>
	<th style="width: 37px;"><span style="font-size:10px;">EST #</span></th>
	<th style="width: 66px; text-align: center;"><span style="font-size:10px;">Production<br />Date</span></th>
	<th style="width: 50px; text-align: left;"><span style="font-size:10px;">Actual<br />Case<br />Count</span></th>
	<th style="width: 60px; text-align: left;"><span style="font-size:10px;">Net<br />Weight</span></th>
	</tr>
</thead>
</#if><tr><#assign boxid = ""><#assign shippingmark = ""><#assign est = ""><#assign casecount += item.custcol_pcg_tfi_actual_case_count><#assign netweight += item.custcol_cw_catch_weight><#if record.custpage_po_data?has_content><#assign pinfo = record.custpage_po_data?eval /><#list pinfo as pay><#if pay.lineno = item.line><#assign boxid = pay.boxid><#assign shippingmark = pay.shippingmark><#assign est = pay.est></#if></#list></#if>
	<td colspan="3" style="width: 100px;"><span style="font-size:9px;">${item.custcol_tfi_item_number}<br /><br />${item.inventorydetail}</span></td>
	<td colspan="3" style="width: 250px;"><span style="font-size:9px;">${item.custcol_scm_item_parent}</span></td>
	<td style="width: 60px;"><span style="font-size:9px;">${boxid}</span></td>
	<td style="width: 70px;"><span style="font-size:9px;">${shippingmark}</span></td>
	<td style="width: 37px;"><span style="font-size:9px;">${est}</span></td>
	<td style="width: 66px;"><span style="font-size:9px;">${item.expectedreceiptdate}</span></td>
	<td align="right" style="width: 50px;"><span style="font-size:9px;">${item.custcol_pcg_tfi_actual_case_count?string.number}</span></td>
	<td align="right" style="width: 60px;"><span style="font-size:9px;">${item.custcol_cw_catch_weight?string.number}</span></td>
	</tr>
	</#list><!-- end items --></table>
</#if>

<table style="break-inside: avoid; margin-top: 10px;"><tr>
	<td colspan="3" style="width: 100px;">&nbsp;</td>
	<td colspan="3" style="width: 250px;">&nbsp;</td>
	<td style="width: 60px;">&nbsp;</td>
	<td style="width: 70px;">&nbsp;</td>
	<td style="width: 37px;">&nbsp;</td>
	<td style="width: 66px;">TOTALS</td>
	<td align="right" style="width: 50px; font-weight: bold; color: #333333;"><span style="font-size:9px;">${casecount?string.number}</span></td>
	<td align="right" style="width: 60px;font-weight: bold; color: #333333;"><span style="font-size:9px;">${netweight?string.number}</span></td>
	</tr></table>
<#if record.expense?has_content></#if>

<hr />
</body>
</pdf>