netsuite.emailtemplate standardemailtemplate {
  scriptid = "standardemailtemplate"
  addcompanyaddress = false
  addunsubscribelink = false
  isinactive = false
  isprivate = false
  name = "Custom Transaction Email Template"
  recordtype = "TRANSACTION"
  subject = "\${companyInformation.companyName}: \${transaction@title} #\${transaction.tranId}"
  usesmedia = false
  content = file("netsuite/emailtemplate/standardemailtemplate.html")
}
